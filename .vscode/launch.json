			"name": "Java: Remote Attach",
			"type": "java",
			"request": "attach",
			"port": 9005,
			"hostName": "localhost"
		},
		{
			"type": "java",
			"name": "Tanzu: Debug Remote Config",
			"request": "attach",
			"port": 9005,
			"hostName": "localhost",
			"preLaunchTask": "tanzuWorkload: Deploy And Connect sample-app-java",
			"preRestartTask": "tanzuWorkload: Deploy And Connect sample-app-java",
			"postDebugTask": "tanzuManagement: Kill Port Forward"
		},
		{
			"type": "java",
			"name": "Launch Current File",
			"request": "launch",
			"mainClass": "${file}"
		},
		{
			"type": "java",
			"name": "Launch Application",
			"request": "launch",
			"mainClass": "com.example.springboot.Application",
			"projectName": "demo"
		}
	]
